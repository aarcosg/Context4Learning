<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2.0dip" />
            <gradient android:startColor="#ff00acc2" android:endColor="#ff00bcd4" android:angle="270.0" android:centerY="0.5" />
            <stroke android:width="1.0px" android:color="#ff000000" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <padding android:left="1.0px" android:top="1.0px" android:right="1.0px" android:bottom="1.0px" />
                <gradient android:startColor="#ff008388" android:endColor="#ff006064" android:angle="90.0" />
                <stroke android:width="1.0px" android:color="#ff000000" />
            </shape>
        </clip>
    </item>
</layer-list>